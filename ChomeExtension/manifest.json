{
    "manifest_version": 3,
    "name": "HTML Table Modifier",
    "version": "2.0",
    "description": "Modify table dimensions and open in new tab - works even with print dialog open",
    "permissions": [
      "activeTab",
      "scripting",
      "tabs"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_title": "Modify Table Dimensions"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }